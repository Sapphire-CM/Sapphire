<tr>
  <td><%= File.basename submission_asset.file.to_s %></td>
  <td>
    <% if submission_asset.submitted_at.present? %>
      <%= l submission_asset.submitted_at, format: :unix %>
    <% else %>
      unknown
    <% end %>
  </td>
  <td><%= submission_asset.content_type %></td>
  <td><%= link_to "view raw", submission_asset_path(submission_asset), class: "small button", target: "_blank" %></td>
</tr>