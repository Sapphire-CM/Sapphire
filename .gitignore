.DS_Store

# Ignore bundler config
/.bundle

# Ignore debugger history
/.byebug_history

# Ignore database.yml because this file depends on the current setup
/config/database.yml

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# Ignore storage files
/exports
/persistent
/uploads

# Ignore code testing coverage
/coverage

# Ignore all database dumps
/db/dumps/*.sql
/db/dumps/*.sql.gz

# Ignore ERD-Diagram generated by rails_erd
/erd.pdf

# rerun informations for cucumber
/rerun.txt

# Ignore textmate configuration
/.tm_properties

# Ignore SublimeText files
*.sublime-project
*.sublime-workspace

# Ignore custom workspace startup script
/startup.sh
