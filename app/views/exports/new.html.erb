<% title course_term_title(current_term), "New #{@export.present? ? @export.class.model_name.human : "Export"}" %>

<%= render 'terms/sidebar', term: @term %>

<% if @export.present? %>
  <%= render "form", export: @export, export_type: @export_type %>
<% else %>
  <%= render "export_type_selector" %>
<% end %>
